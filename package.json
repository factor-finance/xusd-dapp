{
  "name": "xusd-dapp",
  "version": "0.1.0",
  "private": true,
  "description": "XUSD Dapp",
  "main": "index.js",
  "author": "XUSD.fi",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "per-env",
    "lint": "yarn run lint:js",
    "lint:js": "eslint \"**/*.js\"",
    "start:development": "NODE_ENV=development ETHEREUM_RPC_PROVIDER=http://localhost:8545 next dev",
    "start:production": "NODE_ENV=production next start -p 8080",
    "prettier": "prettier --write \"src/**/*.js\" \"pages/**/*.js\"",
    "prettier:check": "prettier -c \"src/**/*.js\" \"pages/**/*.js\"",
    "prepare": "husky install .husky"
  },
  "dependencies": {
    "@analytics/mixpanel": "^0.3.1",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-export-default-from": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.15.6",
    "@babel/plugin-transform-destructuring": "7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/plugin-transform-object-assign": "7.10.4",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "7.15.3",
    "@ethersproject/providers": "^5.4.3",
    "@ethersproject/units": "^5.0.2",
    "@gnosis.pm/safe-apps-web3-react": "^0.6.2",
    "@sentry/browser": "^5.22.3",
    "@sentry/integrations": "^5.22.3",
    "@sentry/node": "^5.22.3",
    "@sentry/webpack-plugin": "^1.17.2",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/ledger-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "@web3-react/walletlink-connector": "^6.2.8",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-source-maps": "^0.0.3",
    "analytics": "^0.7.15",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-fbt": "^0.20.1",
    "babel-plugin-fbt-runtime": "^0.9.17",
    "babel-plugin-module-resolver": "^4.1.0",
    "classnames": "^2.3.1",
    "dateformat": "^4.6.1",
    "dotenv": "^8.6.0",
    "ethereum-block-by-date": "^1.4.2",
    "ethers": "^5.5.1",
    "fbt-runtime": "^0.9.4",
    "file-loader": "6.2.0",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.41.0",
    "next": "10.2.3",
    "next-cookies": "^2.0.3",
    "per-env": "^1.0.2",
    "postcss-flexbugs-fixes": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.0",
    "pullstate": "^1.23.0",
    "react": "^16.14.0",
    "react-autosize-textarea": "^7.1.0",
    "react-bootstrap": "^1.6.2",
    "react-cookie": "^4.1.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.3.0",
    "react-styl": "^0.0.3",
    "react-toastify": "^6.2.0",
    "sass": "^1.39.2",
    "use-analytics": "^0.0.5"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "components": "./src/components",
            "constants": "./src/constants",
            "utils": "./src/utils",
            "pages": "./src/pages",
            "hoc": "./src/hoc",
            "stores": "./src/stores",
            "hooks": "./src/hooks"
          }
        }
      ],
      "babel-plugin-fbt",
      "babel-plugin-fbt-runtime"
    ]
  },
  "devDependencies": {
    "bootstrap": "4.5.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.2.1",
    "eslint": "^7.6.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-react": "^7.20.5",
    "git-revision-webpack-plugin": "3.0.6",
    "html-webpack-plugin": "4.3.0",
    "husky": "^7.0.2",
    "mini-css-extract-plugin": "0.10.0",
    "opener": "1.5.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pretty-quick": "^3.0.0",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "4.1.0",
    "typeface-lato": "0.0.75",
    "typeface-poppins": "0.0.72",
    "url-loader": "4.1.0",
    "vconsole": "^3.3.4"
  },
  "eslintIgnore": [
    "node_modules",
    "public",
    "out"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "always"
  }
}
